public class ArcaneThirst : QuestClass
{
	static bool isCombatFixed = false;
	static uint eternalTetherID = 195755;
	static int questID = 40009;
	public ArcaneThirst()
	{
		Name = "Arcane Thirst";
		QuestId.Add(questID);
		Step.AddRange(new[] { 1, 1, 6 });
	}
	static void Log(string text)
	{
		Logging.WriteDebug("[Arcane Thirst] " + text);
	}
	static void CombatFixStart()
	{
		if (isCombatFixed)
			return;

		CombatFixEnd();
		isCombatFixed = true;
		wManager.Events.FightEvents.OnFightLoop += OnFightLoop;
		robotManager.Events.ProductEvents.OnProductStopping += OnProductStopping;
		Log("combat fix start");
	}
	static void CombatFixEnd()
	{
		wManager.Events.FightEvents.OnFightLoop -= OnFightLoop;
		robotManager.Events.ProductEvents.OnProductStopping -= OnProductStopping;
		isCombatFixed = false;
		Log("combat fix stop");
	}
	static void OnProductStopping(string productName)
	{
		CombatFixEnd();
	}
	static void OnFightLoop(WoWUnit unit, System.ComponentModel.CancelEventArgs cancelable)
	{
		if (!Questing.Has(questID))
		{
			CombatFixEnd();
			return;
		}
		var me = ObjectManager.Me;
		if (me == null || !me.IsValid || !me.IsAlive)
			return;

		var targ = ObjectManager.Target;
		var mob = Questing.FindUnit(99539, 99524);//, 92307);
		if (mob != null &amp;&amp; mob.IsValid &amp;&amp; mob.IsAlive &amp;&amp; mob.IsAttackable &amp;&amp; !mob.IsMyTarget &amp;&amp; !mob.HaveBuff(eternalTetherID))
		{
			if (targ != null &amp;&amp; targ.IsValid)
			{
				wManager.wManagerSetting.AddBlackList(targ.Guid, 10 * 1000, true);
			}
			Log("change target to " + mob.Name);
			Interact.InteractGameObject(mob.GetBaseAddress);
			Fight.StartFight(mob.Guid);
			cancelable.Cancel = true;
		}
	}
	public override bool Pulse()
	{
		CombatFixStart();
		//var p = new Vector3(1719.244, 4620.049, 123.9465, "None");
		var p = new Vector3(1723.594, 4612.897, 123.7943, "None");
		if (!Questing.Attack(99524, 99539))
			GoToTask.ToPosition(p);

		return true;
	}
	public override bool IsComplete()
	{
		return Questing.ObjectiveComplete(this, 2);
	}
