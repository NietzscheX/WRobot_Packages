    <EasyQuest>
      <Name>Safe Keeping</Name>
      <QuestId>
        <int>44799</int>
      </QuestId>
      <QuestType>OverridePulseCSharpCode</QuestType>
      <QuestClass xsi:type="OverridePulseCSharpCodeEasyQuestClass">
        <Code>		var questId = 44799;
		uint buffId = 229839;
		var safePosition = new Vector3(562.4257, 3839.084, 7.403091, "None");
		var positions = new List&lt;Vector3&gt;() {
			new Vector3(498.1719, 3815.978, 1.493026, "None"),
			new Vector3(535.6912, 3795.393, 1.493381, "None"),
		};
		var npcId = new List&lt;int&gt;() {
			115630, //
			115705, //
			115696, //
			//115703, //
		};
		var progress = WorldQuest.Progress(questId, 1);
		var baseBuffStack = 2;
		var maxBuffStack = 5;
		if (ObjectManager.Me.BuffStack(buffId) &lt; baseBuffStack + maxBuffStack - progress)
		{
			var mobEntry = ObjectManager.GetNearestWoWUnit(ObjectManager.GetWoWUnitByEntry(npcId));
			if (mobEntry != null &amp;&amp; mobEntry.IsValid)
			{
				GoToTask.ToPositionAndIntecractWithNpc(mobEntry.Position, mobEntry.Entry);
				return true;
			}

			bool atPosition = false;
			foreach (var pos in positions)
			{
				if (ObjectManager.Me.Position.DistanceTo(pos) &lt; 5)
				{
					atPosition = true;
					break;
				}
			}
			if (!atPosition)
			{
				var randomPointIndex = Others.Random(0, positions.Count - 1);
				GoToTask.ToPosition(positions[randomPointIndex]);
			}
			return true;
		}

		GoToTask.ToPosition(safePosition);
		return true;</Code>
