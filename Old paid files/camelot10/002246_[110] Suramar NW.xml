      <QuestClass xsi:type="FullCSharpCodeEasyQuestClass">
        <Code>#if VISUAL_STUDIO
using robotManager.Helpful;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;
using wManager.Wow.Bot.Tasks;
using wManager.Wow.Class;
using wManager.Wow.Helpers;
using wManager.Wow.ObjectManager;
using wManager.Wow.Enums;
#endif
public class TheMagicofFlight : QuestClass
{
	static Vector3 _underBridgeCenter = new Vector3(2576.495, 4511.933, 202.0948, "None");
	static Vector3 _bubbleJump = new Vector3(2576.284, 4526.914, 208.8087, "Flying");// new Vector3(2574.386, 4527.762, 208.6287, "Flying");
	static Vector3 _startBalcon = new Vector3(2544.909, 4529.673, 231.8074);
	static Vector3 _startJumpPosition = new Vector3(2566.411, 4514.535, 203.2523, "None");
	static List&lt;Vector3&gt; pathFromBridge = new List&lt;Vector3&gt;()
	{
		new Vector3(2536.274f, 4550.407f, 203.4669f, "None"),
		new Vector3(2535.565f, 4553.547f, 201.7341f, "None"),
		new Vector3(2534.047f, 4560.269f, 200.3249f, "None"),
		new Vector3(2532.309f, 4567.969f, 194.3217f, "None"),
		new Vector3(2530.7f, 4575.096f, 183.8088f, "None"),
		new Vector3(2529.065f, 4582.337f, 168.2459f, "None"),
		new Vector3(2527.67f, 4588.517f, 151.0724f, "Swimming"),
	};
	static float _randomRadius = 0.5f;
	static Vector3 _randomDelta = Vector3.Zero;
	public TheMagicofFlight()
	{
		Name = "The Magic of Flight";
		QuestId.Add(43777);
		Step.AddRange(new[] { 1 });
	}
	static void Log(string text)
	{
		Logging.WriteDebug("[The Magic of Flight] " + text);
	}
	public override bool Pulse()
	{
		JumpUnderBridge();
		//JumpFromBalcon();
		return true;
	}
	bool JumpUnderBridge()
	{
		if (FlightBubble != null)
		{
			MoveInBubble();
			return true;
		}
		if (!Quest.HasQuest(43777))
		{
			Log("need to start quest");
			var p = new Vector3(2571.907, 4515.744, 234.3352);
			GoToTask.ToPosition(p);
			return true;
		}
		if (!IsUnderBridge)
		{
			Log("go under bridge");
			if (GoToTask.ToPosition(_startBalcon))
			{
				Questing.ClickMove(_startJumpPosition);
				Questing.Wait(5);
			}
			return true;
		}
		if (StartBubble != null)
		{
			Log("jump in bubble");
			Questing.ClickMove(_bubbleJump);
			Thread.Sleep(200);
			Move.JumpOrAscend();
			Thread.Sleep(2000);
			return true;
		}
		else
		{
			//Log("to jump position");
			GoToTask.ToPosition(_startJumpPosition);
			MountTask.Mount(true, MountTask.MountCapacity.Ground);
		}
		return true;
	}
	bool JumpFromBalcon()
	{
		if (FlightBubble != null)
		{
			MoveInBubble();
			return true;
		}
		if (!WorldQuest.Has(this))
		{
			Log("need to start quest");
			var p = new Vector3(2571.907, 4515.744, 234.3352);
			GoToTask.ToPosition(p);
			return true;
		}
		if (IsUnderBridge)
		{
			Log("under bridge. move out");
			PulseFromBridge();
			return true;
		}
		if (StartBubble != null)
		{
			Questing.Wait(3);
			Log("jump in bubble");
			Questing.ClickMove(StartBubble.Position);
			Questing.Wait(5);
			return true;
		}
		else
		{
			//Log("to start");
			var p = _startBalcon; // 
			if (GoToTask.ToPosition(p))
			{
				Questing.ClickMove(p);
			}
			MountTask.DismountMount();
		}
		return true;
	}
	public void MoveInBubble()
	{
		var bubble = FlightBubble;
		var artifact = ArcaneArtifact;
		if (artifact != null)
		{
			Log("gather artifact");
			Interact.InteractGameObject(artifact.GetBaseAddress);
			return;
		}
		if (bubble == null)
			return;

		Questing.ClickMove(bubble.Position);
	}
	public WoWUnit FlightBubble
	{
		get
		{
			return ObjectManager.GetObjectWoWUnit().
				Where(u =&gt; u.IsValid &amp;&amp; u.Entry == 110657 &amp;&amp; u.GetMove &amp;&amp; u.GetDistance &lt; 11).
				OrderBy(u =&gt; u.GetDistance).FirstOrDefault();
		}
	}
	public WoWUnit StartBubble
	{
		get
		{
			return ObjectManager.GetObjectWoWUnit().
				//Where(u =&gt; u.IsValid &amp;&amp; u.Entry == 110657 &amp;&amp; !u.GetMove &amp;&amp; u.Position.Z &lt; 230f).
				Where(u =&gt; u.IsValid &amp;&amp; u.Entry == 110657 &amp;&amp; u.Position.DistanceTo(_bubbleJump) &lt; 5).
				OrderBy(u =&gt; u.GetDistance).FirstOrDefault();
		}
	}
	public WoWUnit StartBubble2
	{
		get
		{
			return ObjectManager.GetObjectWoWUnit().
				Where(u =&gt; u.IsValid &amp;&amp; u.Entry == 110657 &amp;&amp; !u.GetMove &amp;&amp; u.Position.Z &lt; 230f).
				OrderBy(u =&gt; u.GetDistance).FirstOrDefault();
		}
	}
	public WoWUnit ArcaneArtifact
	{
		get
		{
			return ObjectManager.GetObjectWoWUnit().
				Where(u =&gt; u.IsValid &amp;&amp; u.Entry == 110664 &amp;&amp; u.GetDistance &lt; 5).
				OrderBy(u =&gt; u.GetDistance).FirstOrDefault();
		}
	}
	public override bool IsComplete()
	{
		return WorldQuest.Complete(this);
	}
	public override bool IsCompleted()
	{
		return false;
	}
	public override bool HasQuest()
	{
		return true;
	}
	public static bool IsUnderBridge
	{
		get
		{
			var pos = ObjectManager.Me.Position;
			return pos.DistanceTo(_underBridgeCenter) &lt; 50 &amp;&amp; pos.Z &lt; _startBalcon.Z;
		}
	}
	public static bool PulseFromBridge()
	{
		Questing.GotoPathFromNear(pathFromBridge);
		return true;
	}
}</Code>
