      <QuestClass xsi:type="FullCSharpCodeEasyQuestClass">
        <Code>#if VISUAL_STUDIO
using robotManager.Helpful;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;
using wManager.Wow.Bot.Tasks;
using wManager.Wow.Class;
using wManager.Wow.Helpers;
using wManager.Wow.ObjectManager;
using wManager.Wow.Enums;
#endif

public sealed class DemonsAmongThemH : QuestClass
{
	static List&lt;Vector3&gt; _hotspots = new List&lt;Vector3&gt;()
	{
		new Vector3(1241.953, -4403.065, 25.22045, "None"),
		new Vector3(1276.446, -4402.395, 26.31695, "None"),
		new Vector3(1218.829, -4419.681, 22.0028, "None"),
	};

	static List&lt;int&gt; _mobs = new List&lt;int&gt;() {
		101100,
	};

	static List&lt;Vector3&gt; _pathToPlatform = new List&lt;Vector3&gt;() {
		new Vector3(1232.051f, -4363.966f, 28.15668f, "None"),
		new Vector3(1237.089f, -4365.387f, 30.36714f, "None"),
		new Vector3(1240.366f, -4366.324f, 32.9772f, "None"),
		new Vector3(1245.095f, -4369.277f, 34.83812f, "None"),
	};

	static Vector3 _platformCenter = new Vector3(1251.985, -4372.858, 34.83509, "None");
	static float _platformRadius = 15f;

	static int _sylvanas = 100866; // http://www.wowhead.com/npc=100866/lady-sylvanas-windrunner

	public DemonsAmongThemH()
	{
		Name = "Demons Among Them (H)";
		QuestId.Add(40983);
		Step.AddRange(new[] {12, 1, 0, 0, 0 });
	}

	public override bool Pulse()
	{
		if (!Questing.ObjectiveComplete(this, 1))
		{
			if (MoveToPlatform())
			{
				if (GoToTask.ToPositionAndIntecractWithNpc(_platformCenter, _sylvanas, 1))
				{
					Questing.CancelCutscene();
				}
			}
			return true;
		}
		if (InPlatform)
		{
			MovementManager.MoveTo(_hotspots[Others.Random(0, _hotspots.Count-1)]);
			Thread.Sleep(3 * 1000);
			return true;
		}
		Questing.Grind(_mobs, _hotspots);
		return true;
	}

	public static bool MoveToPlatform()
	{
		if (InPlatform)
			return true;

		if (GoToTask.ToPosition(_pathToPlatform[0]))
		{
			Questing.Path(_pathToPlatform);
		}

		if (!InPlatform)
			return false;

		return true;
	}

	public static bool MoveFromPlatform()
	{
		if (!InPlatform)
			return true;

		var pathReverse = new List&lt;Vector3&gt;(_pathToPlatform);
		pathReverse.Reverse();
		Questing.Path(pathReverse);
		return true;
	}
	
	public static bool InPlatform
	{
		get
		{
			var myPos = ObjectManager.Me.Position;
			if (myPos.DistanceZ(_platformCenter) &gt; 1)
				return false;

			return myPos.DistanceTo2D(_platformCenter) &lt; _platformRadius;
		}
	}

}</Code>
