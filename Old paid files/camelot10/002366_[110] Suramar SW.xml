			return true;
		}
		Conditions.ForceIgnoreIsAttacked = false;
		wManager.wManagerSetting.CurrentSetting.LootMobs = true;
		if (ItemsManager.GetItemCountById(ammoID) &gt; 0)
		{
			Questing.DisableMount();
			Questing.DisableForm();
			GoToTask.ToPositionAndIntecractWithNpc(ammoCampPosition, cannonID, -1);
			return true;
		}
		if (!Questing.Attack(ammoMobID))
			GoToTask.ToPosition(ammoCampPosition);
		return true;</Code>
