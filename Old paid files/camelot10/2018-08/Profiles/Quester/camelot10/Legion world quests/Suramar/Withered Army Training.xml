        <Code>#if VISUAL_STUDIO
using robotManager.Helpful;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;
using wManager.Wow.Bot.Tasks;
using wManager.Wow.Class;
using wManager.Wow.Helpers;
using wManager.Wow.ObjectManager;
using wManager.Wow.Enums;
#endif

public class WitheredArmyTraining : QuestClass
{
	const int MIN_ANCIENT_MANA = 400;

	public WitheredArmyTraining()
	{
		Name = "Withered Army Training";
		QuestId.Add(43943);
		Step.AddRange(new[] { 0 });
	}
	public static void Log(string text)
	{
		Logging.Write("[Withered Army Training] " + text);
	}
	public override bool Pulse()
	{
		var p = new Vector3(1720.384, 4610.655, 123.8034, "None");
		var npc = 97140; //thalyssra
		if (GoToTask.ToPositionAndIntecractWithNpc(p, npc, 1))
		{
			if (TryStart(2000, 5))
				return true;

			if (TryStart(1300, 4))
				return true;

			if (TryStart(900, 3))
				return true;

			if (TryStart(650, 2))
				return true;

			if (TryStart(MIN_ANCIENT_MANA, 1))
				return true;
		}
		return true;
	}
	bool TryStart(int ancientManaPrice, int gossip)
	{
		var ancientMana = SuramarQuests.AncientMana;
		if (ancientMana &lt; ancientManaPrice)
			return false;

		Thread.Sleep(Others.Random(1000, 2000));
		Quest.SelectGossipOption(gossip);
		Thread.Sleep(Others.Random(1000, 2000));
		Lua.LuaDoString("StaticPopup1Button1:Click()");
		Log("start scenario " + ancientManaPrice +" #"+gossip);
		Thread.Sleep(Others.Random(5000, 10000));
		return true;
	}
	public override bool IsComplete()
	{
		if (Questing.Scenario.Stage != 0)
			return true;

		if (SuramarQuests.AncientMana &lt; MIN_ANCIENT_MANA)
			return true;

		if (SuramarQuests.AncientManaMax &lt; MIN_ANCIENT_MANA)
			return true;

		return WorldQuest.Complete(this);
	}
	public override bool HasQuest()
	{
		return true;
	}
	public override bool IsCompleted()
	{
		return false;
	}
}</Code>
